{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "setupFiles": [
    "<rootDir>/setup-env.ts"
  ],
  "moduleNameMapper": {
    "^jose$": "<rootDir>/mocks/jose.ts"
  },
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "maxWorkers": 1
}
